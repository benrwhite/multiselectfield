<div class="field $Type $extraClass">
  <label class="left" for="$ID">$Title</label>
  <div class="middleColumn">
    <div class="panel">
      $UnselectedField
    </div>
    <div class="panel">
      <p><button class="add">Add &gt;</button></p>
      <p><button class="remove">&lt; Remove</button></p>
    </div>
    <div class="panel">
      $SelectedField
    </div>
  </div>
</div>
